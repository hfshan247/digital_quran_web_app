  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "check-types": "tsc --noemit",
    "tslint": "tslint -p . -c tslint.json",
    "find:unused": "next-unused"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
